{
  "search.exclude": {
    ".dccache": true,
    ".history": true,
    ".nvimlog": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    ".test": true,
    "coverage": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true,
    "tags.*": true
  },
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "files.exclude": {
    ".dccache": true,
    ".history": true,
    ".nvimlog": true,
    "node_modules": true,
    "package.lock": true,
    "*.log": true,
    ".DS_Store": true,
    "dist": true,
    ".test": true,
    "coverage": true,
    "*.tsbuildinfo": true,
    ".actrc": true,
    "act": true,
    ".yarn": true,
    ".pnp.*": true,
    ".vim": true,
    "tags.*": true
  },
  "eslint.packageManager": "yarn",
  "licenser.author": "Qlever LLC",
  "licenser.license": "AL2",
  "cSpell.words": [
    "Buildx",
    "codeql",
    "copack",
    "copacker",
    "ecoli",
    "fsis",
    "FSQA",
    "GFSI",
    "HACCP",
    "ksuid",
    "lcov",
    "lfdynamic",
    "masterid",
    "paaco",
    "pdfs",
    "pfgia",
    "pino",
    "reaudit",
    "rowguid",
    "Snyk",
    "sonarjs",
    "trellisfw",
    "vdoc",
    "vdocs"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.useESLintClass": true,
  "coverage-gutters.coverageBaseDir": "coverage",
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": true,
  "coverage-gutters.coverageReportFileName": "coverage/**/*.html",
  "coverage-gutters.showGutterCoverage": false,
  "ponicode.testSettings.testLocation.locationType": "Custom",
  "ponicode.testSettings.testLocation.path": "{rootDir}/test/{filePath}/{fileName}.test.{ext}",
  "explorerExclude.backup": null,
  "npm.packageManager": "yarn",
  "debug.javascript.autoAttachFilter": "smart",
  "debug.javascript.breakOnConditionalError": true,
  "debug.javascript.defaultRuntimeExecutable": {
    "node": "yarn",
    "pwa-node": "yarn"
  },
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
    "!**/node_modules/**",
    "**/$KNOWN_TOOLS$/**",
    "!**/yarn-*",
    "!**/ava-*/**/cli.mjs"
  ]
}

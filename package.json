{
  "name": "@qlever-llc/lf-sync",
  "version": "3.1.19",
  "description": "Trellis LaserFiche sync service",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "script:clean-dups": "node dist/scripts/lf/clean-dups.js",
    "script:trash-by-doc-type": "node dist/scripts/lf/trash-by-doc-type.js",
    "script:move-entry": "node dist/scripts/lf/move-entry.js",
    "script:delete-entry": "node dist/scripts/lf/delete-entry.js",
    "script:find-zero-sized": "node dist/scripts/lf/find-zero-sized.js",
    "test": "yarn build test && c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads",
    "test:live": "cross-env NOCK_MODE=live yarn test",
    "test:record": "cross-env NOCK_MODE=record yarn test",
    "lint": "biome"
  },
  "ava": {
    "failFast": false,
    "concurrency": 1,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    },
    "watchMode.ignoredChanges": [
      "test/**/snapshots"
    ]
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      ".pnp.*",
      ".test"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@oada/client": "^5.3.0",
    "@oada/jobs": "^4.7.5",
    "@oada/lib-config": "^4.0.0",
    "@oada/lib-prom": "^4.0.1",
    "@oada/list-lib": "^6.1.0",
    "@oada/pino-debug": "^4.0.1",
    "@oada/types": "^3.5.3",
    "bs58": "^6.0.0",
    "convict": "^6.2.4",
    "cron": "^4.3.1",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.5.2",
    "csv-writer": "^1.6.0",
    "csvjson": "^5.1.0",
    "debug": "^4.4.1",
    "deep-equal": "^2.2.3",
    "dotenv": "^16.5.0",
    "es-main": "^1.3.0",
    "exponential-backoff": "^3.1.2",
    "formdata-node": "^6.0.3",
    "got": "^14.4.7",
    "json-ptr": "^3.1.1",
    "jsonpath": "^1.1.1",
    "ksuid": "^3.0.0",
    "mssql": "^11.0.1",
    "mysql2": "^3.14.1",
    "p-queue": "^8.1.0",
    "p-timeout": "^6.1.4",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@biomejs/biome": "^1.9.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/convict": "^6.1.6",
    "@types/cron": "^2.4.3",
    "@types/debug": "^4.1.12",
    "@types/deep-equal": "^1.0.4",
    "@types/mssql": "^9.1.7",
    "@types/node": "^22.15.32",
    "@yarnpkg/sdks": "^3.2.2",
    "ava": "6.4.0",
    "ava-nock": "^2.1.0",
    "browserslist": "^4.25.0",
    "c8": "^10.1.3",
    "cross-env": "^7.0.3",
    "tslib": "^2.8.1",
    "type-fest": "^4.41.0",
    "typescript": "5.8.3",
    "update-browserslist-db": "^1.1.3"
  },
  "resolutions": {
    "@oada/lib-prom": "^4.0.1",
    "cross-spawn": "^7.0.5",
    "jsonpath-plus": "^10.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "packageManager": "yarn@4.9.2",
  "volta": {
    "node": "22.9.0"
  }
}
